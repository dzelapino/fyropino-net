@model List<fyropinonet.Models.Task>

<div>
    <div>
        <div>
            <h1>Task List</h1>
        </div>
    </div>
</div>



<div>
    @foreach (var obj in Model.OrderBy(x => x.Name))
    {
    <div>
        <td>@obj.Name</td>
        <a class="btn btn-outline-primary" asp-controller="Task" asp-action="Edit" asp-route-id="@obj.Id">Edit</a>
    </div>
    }
</div>